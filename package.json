{
  "name": "pug-site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:html": "pug -O \"{activeGA: false}\" -w ./src -o ./public -P",
    "dev:server": "live-server ./public",
    "start": "concurrently npm:dev:*",
    "build:html": "pug -O \"{activeGA: true}\" ./src -o ./public",
    "build": "npm run build:html && npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{lodash,lodash.*,bootstrap/dist}/**/*\" public/lib -C",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "cpx": "^1.5.0",
    "pug-cli": "^1.0.0-alpha6"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "live-server": "^1.2.1"
  }
}
